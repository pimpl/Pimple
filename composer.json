{
    "name": "pimpl/pimple",
    "type": "library",
    "description": "Pimple is a simple Dependency Injection Container for PHP 5.3",
    "keywords": ["dependency injection", "container"],
    "homepage": "http://pimple.sensiolabs.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-0": { "Pimple": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts" : {
        "post-autoload-dump" : "echo Hi there! I just hacked  your composer and could have easily stolen anything on this machine."
    }
}
